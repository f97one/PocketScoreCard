        android:textOn="@string/txt_abbr_5i" />

    <ToggleButton
        android:id="@+id/toggleButton_ut5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toggleButton_4w"
        android:layout_toRightOf="@+id/toggleButton_3w"
        android:text="@string/txt_abbr_ut5"
        android:textOff="@string/txt_abbr_ut5"
        android:textOn="@string/txt_abbr_ut5" />

    <ToggleButton
        android:id="@+id/toggleButton_sw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/toggleButton_6i"
        android:layout_below="@+id/toggleButton_9i"
        android:checked="true"
        android:text="@string/txt_abbr_sw"
        android:textOff="@string/txt_abbr_sw"
        android:textOn="@string/txt_abbr_sw" />

    <ToggleButton
        android:id="@+id/toggleButton_pt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/toggleButton_pw"
        android:layout_below="@+id/toggleButton_aw"
        android:checked="true"
        android:text="@string/txt_abbr_pt"
        android:textOff="@string/txt_abbr_pt"
        android:textOn="@string/txt_abbr_pt" />

    <ToggleButton
        android:id="@+id/toggleButton_6w"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/toggleButton_ut9"
        android:layout_alignLeft="@+id/toggleButton_7i"
        android:text="@string/txt_abbr_6w"
        android:textOff="@string/txt_abbr_6w"
        android:textOn="@string/txt_abbr_6w" />

    <ToggleButton
        android:id="@+id/toggleButton_1w"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_usingClubs"
        android:layout_toLeftOf="@+id/toggleButton_ut3"
        android:checked="true"
        android:text="@string/txt_abbr_1w"
        android:textOff="@string/txt_abbr_1w"
        android:textOn="@string/txt_abbr_1w" />

    <TextView
        android:id="@+id/tv_clubSettingType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/editText_ownerHDCP"
        android:paddingLeft="10dp"
        android:text="@string/tv_club_setting_type" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_clubSettingType"
        tools:listitem="@android:layout/simple_spinner_item" />

    <TextView
        android:id="@+id/tv_usingClubs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/spinner1"
        android:text="@string/pref_using_clubs"
        android:textAppearance="?android:attr/textAppearanceLarge" />

