        android:layout_alignLeft="@+id/spinner_par06"
        android:layout_alignTop="@+id/spinner_hdcp07"
        android:layout_toLeftOf="@+id/spinner_hdcp07"
        tools:listitem="@android:layout/simple_spinner_item" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/spinner_par07"
        android:layout_alignBottom="@+id/spinner_par07"
        android:layout_alignParentLeft="true"
        android:text="@string/tv_hole7"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Spinner
        android:id="@+id/spinner_hdcp08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/spinner_hdcp07"
        android:layout_toRightOf="@+id/spinner_par07"
        android:entries="@array/HdcpValue"
        tools:listitem="@android:layout/simple_spinner_item" />

    <Spinner
        android:id="@+id/spinner_par08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/spinner_par07"
        android:layout_alignRight="@+id/spinner_par07"
        android:layout_alignTop="@+id/spinner_hdcp08"
        android:entries="@array/ParValue"
        tools:listitem="@android:layout/simple_spinner_item" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/spinner_par08"
        android:layout_alignBottom="@+id/spinner_par08"
        android:layout_alignParentLeft="true"
        android:text="@string/tv_hole8"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>