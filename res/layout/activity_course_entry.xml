        android:layout_below="@+id/spinner_hdcp06"
        android:layout_toRightOf="@+id/spinner_par06"
        android:entries="@array/HdcpValue"
        tools:listitem="@android:layout/simple_spinner_item" />

    <Spinner
        android:id="@+id/spinner_par07"
        android:layout_width="wrap_content"
