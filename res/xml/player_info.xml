    </PreferenceCategory>
    <PreferenceCategory android:title="@string/pref_using_clubs" >
        <PreferenceCategory android:title="@string/pref_club_woods" >
            <CheckBoxPreference
                android:defaultValue="true"
                android:key="pref_club_1w" />
            <CheckBoxPreference
                android:defaultValue="false"
                android:key="pref_club_3w" />
            <CheckBoxPreference android:key="pref_club_4w" />
            <CheckBoxPreference android:key="pref_club_5w" />
            <CheckBoxPreference android:key="pref_club_6w" />
            <CheckBoxPreference android:key="pref_club_7w" />
