        <PreferenceCategory android:title="@string/pref_club_wedges" >
            <CheckBoxPreference android:key="pref_club_pw" />
            <CheckBoxPreference android:key="pref_club_aw" />
            <CheckBoxPreference android:key="pref_club_sw" />
            <CheckBoxPreference android:key="pref_club_lw" />
